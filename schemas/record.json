{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "record",
  "type": "object",
  "definitions": {
    "record": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "datetime": {
          "type": "string",
          "description": "datetime string describing time of record."
        },
        "image": {
          "type": "image.json#definitions/image",
        },
        "classification": {
          "type": "classification.json#definitions/classification"
        },
        "extra": {
          "type": "string",
          "description": "Any extra data to include"
        }
      }
    }
  }
}
